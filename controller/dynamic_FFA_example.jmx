          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import org.apache.jmeter.testelement.property.CollectionProperty;
import org.apache.jmeter.testelement.property.PropertyIterator;

data = ctx.getThreadGroup().getData();
//log.warn(data.toString())

PropertyIterator it = data.iterator();
while (it.hasNext()) {
	CollectionProperty record = (CollectionProperty) it.next();
	record.set(0, &quot;33&quot;);
	record.set(1, &quot;33&quot;);
}
</stringProp>
